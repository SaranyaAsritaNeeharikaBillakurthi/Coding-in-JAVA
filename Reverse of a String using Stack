import java.util.*;
class Reversestack
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
Stack <Character> st=new Stack<Character>();
System.out.println("Enter string:");
String str=sc.next();
char[] ch = str.toCharArray();
int n = ch.length;
for(int i=0;i<n;i++)
{
st.push(ch[i]);
}
for(int i=0;i<n;i++)
System.out.print(st.pop());
}
}
