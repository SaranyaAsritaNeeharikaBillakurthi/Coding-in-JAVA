// https://practice.geeksforgeeks.org/problems/multiply-left-and-right-array-sum1555/1

class Complete
{
    public static int multiply (int arr[], int n)
    {
        int sum1=0,sum2=0;
        for(int i=0;i<n;i++)
        {
            if(i>=0 && i<n/2)
            {
                sum1=sum1+arr[i];
            }
            else
            sum2=sum2+arr[i];
        }
        return sum1*sum2;
    }
}
