import java.util.Scanner;
class Strprogram2
{
public static void main(String args[])
{
int sum=0;
String str;
Scanner sc=new Scanner(System.in);
str=sc.nextLine();
String ch[]=str.split(" ");
int n=ch.length;
if(n!=0)
{
for(int i=n-1;i>=0;i--)
{
String s=ch[i];
int m=s.length();
sum=sum+m;
}
System.out.println(sum);
}
else
System.out.println(n);
}
}
