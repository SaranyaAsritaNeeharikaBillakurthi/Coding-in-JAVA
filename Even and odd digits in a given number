import java.util.*;
class Eevenodd
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
int n;
int r,even=0,odd=0;
int even_count=1,odd_count=1;
n=sc.nextInt();
while(n!=0)
{
r=n%10;
n=n/10;
if(r%2 == 0)
{
even=r*even_count+even;
even_count=even_count&10;
}
else
{
odd=r*odd_count+odd;
odd_count=odd_count*10;
}
}
System.out.println(even+" "+odd);
}
}
